ShowStartTime=22:00

#Various config items
FilterByUser=tmbeuxxxx
SecondsBeetweenChecks=60
ReportSaveLocation=c:\temp
TemplateID=895325

#Valid formats are: pdf, html (a zip file), mht, xml, csv, or docx
ReportFormat=csv

